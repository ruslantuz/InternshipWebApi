@model List<GroupModel>
@{
    ViewBag.Title = "Groups";
}
<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Major Subject</th>
            <th>Year</th>
            <th>Student count</th>
        </tr>
    </thead>
    <tbody>
        @foreach (GroupModel group in @Model)
        {
            <tr>
                <td>@group.Id</td>
                <td>@group.Name</td>
                <td>@group.MajorSubject</td>
                <td>@group.Year</td>
                <td>@group.StudentCount</td>
            </tr>
        }
    </tbody>
</table>
